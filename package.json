{
  "name": "maki-swap",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "migrate": "truffle migrate --reset",
    "migrate:testnet": "truffle migrate --reset --network hecotest",
    "compile": "truffle compile",
    "compile:testnet": "truffle compile --network hecotest",
    "test": "npx buidler test",
    "coverage": "npx buidler coverage",
    "clean": "rimraf ./build/",
    "verify:factory": "truffle run verify MakiswapFactory --network hecotest --license SPDX-License-Identifier",
    "verify:router": "truffle run verify MakiswapRouter --network hecotest --license SPDX-License-Identifier",
    "verify:multicall": "truffle run verify Multicall --network hecotest --license SPDX-License-Identifier",
    "flat:factory": "truffle-flattener ./contracts/MakiswapFactory.sol > ./flats/MakiswapFactory_flat.sol",
    "flat:router": "truffle-flattener ./contracts/MakiswapRouter.sol > ./flats/MakiswapRouter_flat.sol",
    "flatten:multicall": "truffle-flattener ./contracts/Multicall.sol > ./flats/Multicall_flat.sol"
  },
  "dependencies": {
    "@openzeppelin/test-helpers": "^0.5.11",
    "@truffle/hdwallet-provider": "^1.3.0",
    "ethers": "^5.1.4",
    "maki-swap-lib": "^2.1.0",
    "maki-swap-periphery": "^3.0.0",
    "makiswap-core": "^4.1.0",
    "solidity-coverage": "^0.7.16",
    "truffle": "^5.1.58",
    "truffle-flattener": "^1.4.4",
    "truffle-verify": "^1.0.8"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.8",
    "@nomiclabs/buidler-ethers": "^2.0.2",
    "@nomiclabs/buidler-truffle5": "^1.3.4",
    "@nomiclabs/buidler-waffle": "^2.1.0",
    "@nomiclabs/buidler-web3": "^1.3.4",
    "@types/chai": "^4.2.6",
    "@types/mocha": "^5.2.7",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.3.0",
    "ethereumjs-util": "^6.2.0",
    "mocha": "^6.2.2",
    "rimraf": "^3.0.0",
    "solc": "^0.5.12",
    "truffle-hdwallet-provider": "^1.0.17",
    "truffle-plugin-verify": "^0.5.7",
    "web3": "^1.3.4"
  }
}
